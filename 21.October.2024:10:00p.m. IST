int countgroup(vector<int>& arr) {
        // Complete the function
         // Complete the function
        int mod = 1e9+7;
        int xor_ =0;
        int n = arr.size();
        for(int i = 0 ;i <n;i++)
            xor_^=arr[i];
        if(xor_!=0)
            return 0;
        return ((int)(pow(2,n-1))-1)%mod;
    }
